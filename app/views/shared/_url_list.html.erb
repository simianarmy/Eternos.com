<% unless @feed_urls.empty? %>
  <br>
  <h5>List of your URLs</h5>
  <table class="table-form2" align="left">
    <% @feed_urls.each do |@feed| -%>
      <tr id="url-list-<%= @feed.id %>">
        <td valign="right"></td>
        <td valign="center">:</td>
        <td valign="right"><%= in_place_editor_field(:feed, :rss_url) %>
        </td>
        <td valign="left"><%= link_to_remote 'Remove', 
                                                      :html => {:class => 'blue-btn4'},
                                                      :url => {:controller => 'account_settings', :action => 'online', :id => @feed},
                                                      :confirm => 'Are you sure?',
                                                      :with => "'method=delete'", 
                                                      :loading => "$('progress-bar').show();",
                                                      :complete => "$('progress-bar').hide();" %>    
        </td>
      </tr>
    <% end %>
  </table>
  <br>
<%= will_paginate(@feed_urls) %>
<% end %>
