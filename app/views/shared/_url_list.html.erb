<% unless @feed_urls.empty? %>
  <br>
  <h5>List of RSS Feeds to Backup</h5>
  <table class="table-form6" border="0" cellpadding="0" cellspacing="0">
      <tr bgcolor="#8bcdff">
        <td class="coll-form10" bgcolor="#8bcdff" align="center">URL</td>
        <td class="coll-form10" bgcolor="#8bcdff" align="center">Action</td>
      </tr>
    <% @feed_urls.each do |@feed| -%>
      <tr id="url-list-<%= @feed.id %>">
        <td class="coll-form12" valign="center"><%= in_place_editor_field(:feed, :rss_url) %></td>
        <td class="coll-form12" valign="center"><%= link_to_remote 'Remove', 
                                                      :html => {:class => 'blue-btn4'},
                                                      :url => {:controller => 'backup_sources', :action => 'remove_url', :id => @feed},
                                                      :confirm => 'Are you sure?',
                                                      :loading => "$('progress-bar').show();",
                                                      :complete => "$('progress-bar').hide();" %>    
        </td>
      </tr>
    <% end %>
  </table>
  <br>
<%= will_paginate(@feed_urls) %>
<% end %>
