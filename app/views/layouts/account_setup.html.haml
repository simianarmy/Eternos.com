!!!
%html{html_attrs('en'), 'xmlns:fb' => "http://www.facebook.com/2008/fbml"}
  %head
    %link{:rel=>"shortcut icon", :type=>"image/vnd.microsoft.icon", :href=>"#{image_path 'favicon.ico'}"}
    %link{:rel=>"icon", :type=>"image/png", :href=>"#{image_path 'favico.png'}"}
    %meta{:'http-equiv' => "X-UA-Compatible", :content => "IE=8"}
    %meta{:'http-equiv'=>"Content-Type", :content=>"text/html; charset=utf-8"}
    %meta{:name=>"keywords", :content=>"#{meta_keywords}"}
    %meta{:name=>"description", :content=>"#{meta_descriptions}"}
    %title= "Eternos Members - " + h(yield(:title) || "Home")
    / bundle do
    - stylesheet 'screen', 'account_settings', 'application'
    - load_prototype
    - use_lightview
    - use_calendar
    - use_busy
    - use_prototip
    - use_form
    
    - javascript 'application', 'account_setup'
      
    = yield(:head)
    = yield(:js_libs)
    = yield(:javascript)
    
    / end bundle
  %body
    #wrapper
      #header
        #logo
          %h1= link_to((image_tag 'eternos.gif', :id => 'eternos_logo', :border => '0'),  root_path)
  
      #content
        #sub_content
          = yield
    
    = render :partial => 'shared/get_satisfaction'
    