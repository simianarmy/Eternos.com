!!!
%html{html_attrs('en'), 'xmlns:fb' => "http://www.facebook.com/2008/fbml"}
  %head
    %link{:rel=>"icon", :type=>"image/png", :href=>"#{image_path 'favico.png'}"}
    %meta{:'http-equiv' => "X-UA-Compatible", :content => "IE=8"}
    %meta{:'http-equiv'=>"Content-Type", :content=>"text/html; charset=utf-8"}
    %title= "Eternos - " + h(yield(:title) || "Timeline Events")
    / bundle do
    - stylesheet 'screen', 'application', 'timeline_details.css'
    - load_prototype
    - use_busy
    - use_scrollbar
    - use_lowpro
    - use_prototip
    - javascript 'application'
    
    = yield(:head)
    = yield(:js_libs)
    = yield(:javascript)
    / end bundle
  %body
    .popup-container
      .popuptop
      .popup-content
        .top-box2
          .acc-title Timeline Item Details
          .clearboth
        .clearboth
        #notice.flash_notice= show_flash_messages
        = yield
      = image_tag 'popup-footer.png'

      