<% unless list.blank? %>
  <br>
  <div class="title7"><b>Accounts Added</b>:</div>
  <table class="table-form8" border="0" cellpadding="0" cellspacing="0">
      <tr bgcolor="#b3ecef">
        <td class="coll-form15" bgcolor="#94e4e8" align="center">Screen name</td>
        <td class="coll-form15" bgcolor="#94e4e8" align="center">Action</td>
      </tr>
    <% list.each do |twitter| -%>
      <tr id="twitter-list-<%= twitter.id %>">
        <td class="coll-form12" valign="center"><%= twitter.title %></td>
        <td class="coll-form12" valign="center"><%= link_to_remote 'Remove', 
        :html => {:class => 'blue-btn4'},
        :url => remove_twitter_account_backup_sources_path(:id => twitter.id),
        :confirm => 'Are you sure?',
        :with => "'method=delete'", 
        :loading => "$('progress-bar').show();",
        :complete => "$('progress-bar').hide();" %>    
        </td>
      </tr>
    <% end %>
  </table>
  <br>
<% end %>
