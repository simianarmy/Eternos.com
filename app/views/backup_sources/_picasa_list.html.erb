<% unless list.blank? %>
  <br/>
  <table class="backup-accounts-table">
    <tr>
      <th>Owner</th>
      <th>Authorization Status</th>
      <th>Actions</th>
    </tr>
    <% list.each do |picasa| -%>
      <tr id="picasa-list-<%= picasa.id %>">
        <td><%= picasa.title %></td>
        <td><%= picasa.active? ? 'Active' : 'Inactive' %></td>
        <td><%= link_to_remote 'Remove', 
        :url => {:controller => 'backup_sources', :action => 'remove_picasa_account', :id => picasa},
        :confirm => account_delete_confirmation,
        :with => "'method=delete'", 
        :loading => "$('progress-bar').show();",
        :complete => "$('progress-bar').hide();" %>    
        </td>
      </tr>
    <% end %>
  </table>
  <br>
<% end %>
