- use_calendar
- title 'Account Setup'

.content.login1
  %fieldset
    .login1_left
      %h1 Account Setup
      %ul.signup-left
        %li{:class => get_signup_wizard_step_class(@completed_steps >= 1)}
          %strong Step 1
          %br
          %span Online Accounts
        %li{:class => get_signup_wizard_step_class(@completed_steps > 2)}
          - if @completed_steps >= 2
            %strong Step 2
          - else 
            Step 2
          %br
          %span Invite Friends
          
    .login1_right.login1-mid.login2-mid
      = render :partial => @content_page, :locals => {:settings => @settings}
      